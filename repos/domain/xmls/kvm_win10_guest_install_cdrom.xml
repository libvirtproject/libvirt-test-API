<domain type="kvm">
  <name>GUESTNAME</name>
  <memory>MEMORY</memory>
  <vcpu>VCPU</vcpu>
  <os>
    <type machine="GUESTMACHINE">hvm</type>
    <boot dev="cdrom"/>
  </os>
  <cpu mode='custom' match='exact'>
    <model fallback='allow'>IvyBridge</model>
  </cpu>
  <features>
    <acpi/>
    <apic/>
    <pae/>
  </features>
  <clock offset="utc"/>
  <on_poweroff>destroy</on_poweroff>
  <on_reboot>restart</on_reboot>
  <on_crash>restart</on_crash>
  <devices>
    <disk device="disk" type="file">
      <driver name="qemu" type='IMAGEFORMAT'/>
      <source file='DISKPATH'/>
      <target bus='HDDRIVER' dev='DEV'/>
    </disk>
    <disk device="cdrom" type="file">
      <source file='WINDOWSISO'/>
      <target bus="ide" dev="hdc"/>
      <readonly/>
    </disk>
    <disk type="file" device='floppy'>
      <driver name='qemu' type='raw'/>
      <source file='DRIVERPATH'/>
      <target dev='fda' bus='fdc'/>
    </disk>
    <disk device="floppy" type="file">
      <source file='FLOPPY'/>
      <target bus="fdc" dev="fdb"/>
      <readonly/>
    </disk>
    <interface type="network">
      <source network="default"/>
      <mac address='MACADDR'/>
      <model type='NICDRIVER'/>
    </interface>
    <console/>
    <input bus="ps2" type="mouse"/>
    <graphics port="-1" autoport="yes" type="GRAPHIC"/>
    <video>
      <model type='cirrus' vram='16384' heads='1'/>
    </video>
    </devices>
</domain>
